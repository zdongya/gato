<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.yunjing.entity.WarningInfo" table="tb_warning_info" >
        
        <id name="warningId">
            <generator class="assigned" />
        </id>
        <property name="warnDate" />
        <property name="warnType" />
        <property name="memo" />
        <property name="istate"/>
        <many-to-one name="zone" column="zoneNo" class="com.yunjing.entity.Zone" fetch="select"/> 
        <many-to-one name="operator" column="handler" class="com.yunjing.entity.Member" fetch="select"/>
		<property name="handleDate" />
    </class>
</hibernate-mapping>
